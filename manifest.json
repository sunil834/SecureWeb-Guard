{
  "manifest_version": 3,
  "name": "Web-Guard",
  "version": "1.0",
  "description": "Alerts you from phishing sites by checking URLs against a known list.",
  "permissions": [
    "tabs",
    "scripting",
    "activeTab",
    "storage",
    "notifications"
  ],
  "host_permissions": [
    "https://raw.githubusercontent.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icons/icon.png"
    }
  },
  "icons": {
    "128": "icons/icon.png"
  }
}
